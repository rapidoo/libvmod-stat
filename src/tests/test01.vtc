varnishtest "Test stat vmod"

server s1 {
       rxreq
       txresp
} -start

varnish v1 -vcl+backend {
	import stat from "${vmod_topbuild}/src/.libs/libvmod_stat.so";

	sub vcl_deliver {
		set resp.http.hello = stat.getvisitorid("312");
	}
} -start

client c1 {
	txreq -url "/"
	rxresp
	expect resp.http.hello == "Hello, World"
}

client c1 -run
